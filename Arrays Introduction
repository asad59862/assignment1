#include <iostream>
using namespace std;

void print(int arr[], int length){
     for(int i = length - 1; i >= 0; i--){
          cout << arr[i] << " ";
           }
         }

int main() { 
    int length;
     cin >> length;

int arr[length]; 

for(int i = 0; i < length; i++){
    cin >> arr[i];
}

print(arr, length);

return 0;
}
